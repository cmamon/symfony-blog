{% extends 'base.html.twig' %}

{% block title %}Post list{% endblock %}

{% block body %}
    {% for post in posts %}
    [<br>
        &emsp;<b>{{ post.name }}</b> <br>
        &emsp;{{ post.slug }} <br>
        &emsp;{{ post.publicationDate.format('d-m-Y H:i:s') }} <br>
        &emsp;{{ post.content }} <br>
    ]<br>
    <br>
    {% endfor %}
{% endblock %}
